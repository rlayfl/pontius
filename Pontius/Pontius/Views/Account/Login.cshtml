@{
    ViewData["Title"] = "Login";
}


<form id="loginForm" onsubmit="login(); return false;">

    <div class="form-group"></div>
        <label for="emailAddress">Unique Identifier</label>
        <input type="text" class="form-control" id="emailAddress" name="emailAddress" value="email@email.com" required />

        <label for="password">Password</label>
        <input type="password" class="form-control" id="password" name="password" value="testpassword" required />
        <button type="submit" class="btn btn-primary">Login</button>
    </div>

    

</form>

<script src="/js/account/login.js"></script>